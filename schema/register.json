{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "token": {
      "type": "string"
    },
    "error": {
      "type": "string"
    }
  },
  "anyOf": [
    { "required": [ "id", "token" ] },
    { "required": [ "error" ] }
  ]
}